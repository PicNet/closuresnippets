<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>jsextends</Title>
    <Author>PicNet Pty Ltd</Author>
    <Shortcut>jsextends</Shortcut>
    <Description>An extends statement.</Description>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
  </Header>
  <Snippet>
    <Declarations>
			<Literal>
        <ID>namespace</ID>
        <ToolTip>The name of this class' namespace</ToolTip>
        <Default>NameSpace</Default>
      </Literal>
			<Literal>
        <ID>inherits</ID>
        <ToolTip>The name of the inherited class</ToolTip>
        <Default>Inherits</Default>
      </Literal>		
    </Declarations>
    <Code Language="jscript"><![CDATA[* @extends {$namespace$.$inherits$}
]]></Code>
  </Snippet>
</CodeSnippet>