<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>jstypedef</Title>
    <Author>PicNet Pty Ltd</Author>
    <Shortcut>jstypedef</Shortcut>
    <Description>A typedef statement.</Description>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
  </Header>
  <Snippet>
    <Declarations>
			<Literal>
        <ID>namespace</ID>
        <ToolTip>The name of this class' namespace</ToolTip>
        <Default>NameSpace</Default>
      </Literal>
			<Literal>
        <ID>type</ID>
        <ToolTip>The composite type of this definition</ToolTip>
        <Default>(string|number)</Default>
      </Literal>
      <Literal>
        <ID>typedefname</ID>
        <ToolTip>The name of this typedef</ToolTip>
        <Default>TypeDefName</Default>
      </Literal>						
    </Declarations>
    <Code Language="jscript"><![CDATA[/** @typedef {$type$} */
$namespace$.$typedefname$;
]]></Code>
  </Snippet>
</CodeSnippet>